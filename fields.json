{
	"field":
	{
		"division":
		{
			"field_regex": "\\+{3}"
		},
		"subdivision":
		{
			"field_regex": "\\*{3}"
		}
	},
	"title":
	{
		"name":
		{
			"field_regex": "(^.+)(?=\\s.+$)",
			"match": 0
		},
		"version":
		{
			"field_regex": "(\\w[^\\s]+$)",
			"match": 0
		},
		"url":
