			"field_regex": ".+:[\/]{0,2}.+$",
			"match": 0
		},
		"description":
		{
			"field_regex": "(^.+)",
			"match": 1
		},
		"download_url":
		{
			"field_regex": ".+:[\/]{0,2}.+$",
			"match": 1
		}
	},
	"authorship":
	{
